<div class="EnvironmentalBg">
</div>
<div class="content">
  <div class="title">
    <h2>
      <img src="../../images/icons/icon.png" alt="" />
      <span class="page-title"></span>
    </h2>
  </div>
  <div class="imgContainer clearfix">
    <div class="banner">
      <div class="large_box">
        <span class="left_btn"></span>
        <ul>
          {{#each imageList}}
            <li>
              <img src="{{this.imageUrl}}" data-detail-id="{{this.detailID}}">
            </li>
          {{/each}}
        </ul>
        <span class="right_btn"></span>
      </div>
      <div class="small_box">
        <span class="btn left_btn"></span>
        <div class="small_list">
          <ul>
            {{#each imageList}}
              <li>
                <img src="{{this.imageUrl}}" data-detail-id="{{this.detailID}}">
                <div class="bun_bg"></div>
              </li>
            {{/each}}
          </ul>
        </div>
        <span class="btn right_btn"></span>
      </div>
    </div>

    <div class="Text_introduce">
      <p></p>
    </div>
    <div class="attachment">
      <h2>附件</h2>
      <ul class="clearfix">
        {{#each fileList}}
          <li>
            <a href="{{this.fileUrl}}" target="_blank">{{this.fileName}}</a>
          </li>
        {{/each}}
      </ul>
    </div>
  </div>
</div>
<div class="copyright">
  交通银行渭南分行 服务档案
</div>
<input type="hidden" value="{{itemID}}" id="hidden-itemID">
<input type="hidden" value="{{itemName}}" id="hidden-itemName">
<input type="hidden" value="{{year}}" id="hidden-year">
<input type="hidden" value="{{quarter}}" id="hidden-quarter">
<script src="javascripts/detail.js"></script>